{
  "name": "kernelman/logs",
  "description": "Logs library package for php language.",
  "type": "library",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Kernel Huang",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "kernelman/exceptions" : ">=1.0.0",
    "kernelman/services" : ">=1.0.0",
    "kernelman/message" : ">=1.0.0",
    "kernelman/files" : ">=1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Logs\\Adapter\\": "src/Adapter",
      "Logs\\Processor\\": "src/Processor",
      "Logs\\Services\\": "src/Services",
      "Logs\\Models\\": "src/Models",
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
